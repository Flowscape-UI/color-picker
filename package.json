{
  "name": "@flowscape-ui/design-system-kit",
  "version": "1.0.0",
  "description": "A comprehensive React design system kit with color picker, input range, and other essential UI components. Built with TypeScript and optimized for modern web applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "react",
    "design-system",
    "design-system-kit",
    "ui-components",
    "component-library",
    "color-picker",
    "input-range",
    "color",
    "gradient",
    "react-component",
    "typescript",
    "ui",
    "ui-kit",
    "color-selector",
    "rgb",
    "hsl",
    "hex",
    "cmyk",
    "hsv",
    "eye-dropper",
    "dark-mode",
    "light-mode",
    "tailwindcss",
    "framer-motion"
  ],
  "author": "Flowscape UI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flowscape-ui/design-system-kit"
  },
  "bugs": {
    "url": "https://github.com/flowscape-ui/design-system-kit/issues"
  },
  "homepage": "https://github.com/flowscape-ui/design-system-kit#readme",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsup",
    "test": "vitest run",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook:gh-pages": "STORYBOOK_BASE_HREF=/design-system-kit/ storybook build"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "dom-to-image-more": "^3.7.1",
    "tailwind-merge": "^3.3.1",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.6.2",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/tinycolor2": "^1.4.4",
    "@vitejs/plugin-react": "^5.0.4",
