    "react": ">=18",
    "react-dom": ">=18",
    "react-icons": "^5.5.0",
    "storybook": "^9.1.10",
    "tailwindcss": "^4.1.14",
    "terser": "^5.44.0",
    "tsup": "^8.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
